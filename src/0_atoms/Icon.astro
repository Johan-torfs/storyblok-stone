---
import { storyblokEditable, type SbBlokData } from '@storyblok/astro';

interface StoryblokIcon extends SbBlokData {
    component: "icon";
    icon: {
        type: string;
        icon: string;
    };
}


interface Props {
    blok: StoryblokIcon;
}

const { blok } = Astro.props;
---

<i {...storyblokEditable(blok)} class:list={[blok.icon.type, blok.icon.icon]} aria-hidden="true"></i>